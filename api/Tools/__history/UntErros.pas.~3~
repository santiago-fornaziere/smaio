unit UntErros;

interface

function ErroUsuarioExpirado : TJsonObject;

implementation

uses
  System.JSON;

function ErroUsuarioExpirado : TJsonObject;
var vErro : TJSONObject;
begin
     vErro := TJsonObject.Create;
     vErro.AddPair('mensagem', TJSONString.Create('Data de valiadde expirada'));
     vErro.AddPair('id', TJSONNumber.Create('1'));
end;

end.
